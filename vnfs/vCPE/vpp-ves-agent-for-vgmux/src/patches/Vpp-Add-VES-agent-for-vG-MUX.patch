+VLIB_REGISTER_NODE (ves_agent_process_node, static) = {
+	.function = ves_agent_process,
+	.type = VLIB_NODE_TYPE_PROCESS,
+	.name = "ves-agent-process",
+	.process_log2_n_stack_bytes = 16,
+};
